<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".homePage.Student.Student_homePage"
    android:padding="30dp"

    >

    <TextView
        android:gravity="center"
        android:textSize="20dp"
        android:layout_marginBottom="20dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/admin_home_page_ui">

    </TextView>
    <Button
        android:id="@+id/Announcement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:text="Announcement"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="369dp" />
    <Button
        android:id="@+id/Events"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:text="Events"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="266dp" />

    <Button
        android:id="@+id/Complaints"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:text="Complaints"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="153dp" />


    <Button
        android:id="@+id/POSt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:textAllCaps="false"
        android:text="POSt"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="369dp" />

    <Button
        android:id="@+id/AdminFeature1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:visibility="gone"
        android:text="Button"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="465dp" />

    <Button
        android:id="@+id/AdminFeature2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:visibility="gone"
        android:text="Button"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="574dp" />

    <Button
        android:id="@+id/Logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:text="Logout"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="655dp" />
</LinearLayout>